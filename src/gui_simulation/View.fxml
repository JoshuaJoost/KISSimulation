<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onKeyReleased="#mazeMoveRobot" prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui_simulation.Controller">
   <children>
      <AnchorPane prefHeight="520.0" prefWidth="500.0">
         <children>
            <Label fx:id="mazeLable" prefHeight="20.0" text="Labyrinth: &lt;keines ausgewählt&gt;" />
            <Pane fx:id="mazePane" layoutY="20.0" prefHeight="500.0" prefWidth="500.0" />
         </children>
      </AnchorPane>
      <AnchorPane layoutY="520.0" prefHeight="180.0" prefWidth="500.0">
         <children>
            <Label fx:id="robotSelectedLable" layoutX="50.0" prefHeight="20.0" text="Roboter &lt;keiner ausgewählt&gt;" />
            <Pane fx:id="robotPane" layoutY="20.0" prefHeight="160.0" prefWidth="500.0">
               <children>
                  <TableView fx:id="robotTable" prefHeight="160.0" prefWidth="290.0">
                    <columns>
                      <TableColumn fx:id="robotTableNr" editable="false" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" resizable="false" text="Nr" />
                      <TableColumn fx:id="robotTableName" editable="false" maxWidth="170.0" minWidth="170.0" prefWidth="170.0" resizable="false" text="Name" />
                        <TableColumn fx:id="robotTableSelected" editable="false" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" resizable="false" text="Ausgewählt" />
                    </columns>
                  </TableView>
                  <AnchorPane layoutX="290.0" prefHeight="160.0" prefWidth="210.0">
                     <children>
                        <Button fx:id="robotStartStop" mnemonicParsing="false" onAction="#robotStartStop" text="Start/ Stopp" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
                        <Button fx:id="robotGetMatrix" layoutY="50.0" mnemonicParsing="false" onAction="#robotGetMatrix" text="Runterladen" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
                        <Button fx:id="robotDelete" layoutY="100.0" mnemonicParsing="false" onAction="#robotDelete" text="Löschen" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
                     </children>
                  </AnchorPane>
               </children></Pane>
            <Button fx:id="addNewRobot" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" onAction="#addNewRobot" prefHeight="20.0" prefWidth="20.0" text="+" textOverrun="CLIP">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="500.0" layoutY="200.0" prefHeight="500.0" prefWidth="300.0">
         <children>
            <Label prefHeight="20.0" text="Historie" />
            <Pane fx:id="historyPane" layoutY="20.0" prefHeight="480.0" prefWidth="300.0">
               <children>
                  <TableView fx:id="historyTable" prefHeight="480.0" prefWidth="300.0">
                    <columns>
                      <TableColumn fx:id="historyTableNr" editable="false" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" resizable="false" text="Nr" />
                      <TableColumn fx:id="historyTableAction" editable="false" maxWidth="135.0" minWidth="135.0" prefWidth="135.0" resizable="false" text="Action" />
                        <TableColumn fx:id="historyTableFeedback" editable="false" maxWidth="135.0" minWidth="135.0" prefWidth="135.0" resizable="false" text="Feedback" />
                    </columns>
                  </TableView>
               </children></Pane>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="500.0" prefHeight="200.0" prefWidth="300.0">
         <children>
            <Pane layoutY="20.0" prefHeight="180.0" prefWidth="300.0">
               <children>
                  <TableView fx:id="mazefileTable" onMouseClicked="#mazefileTableSelectRow" prefHeight="180.0" prefWidth="300.0">
                    <columns>
                      <TableColumn fx:id="mazefileTableNr" editable="false" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" resizable="false" sortable="false" text="Nr" />
                      <TableColumn fx:id="mazefileTableFilename" editable="false" maxWidth="180.0" minWidth="180.0" prefWidth="180.0" resizable="false" sortable="false" text="Dateiname" />
                        <TableColumn fx:id="mazefileTableSelected" editable="false" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" resizable="false" sortable="false" text="Ausgewählt" />
                    </columns>
                  </TableView>
               </children></Pane>
            <Label fx:id="mazeLableSelectedFile" prefHeight="20.0" text="Labyrinth Datei" />
         </children>
      </AnchorPane>
   </children>
</Pane>
